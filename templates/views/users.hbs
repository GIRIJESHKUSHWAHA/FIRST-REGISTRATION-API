<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/style.scss" />
    <title>Document</title>
</head>
<script>
    fetch('http://localhost:3000/users/list')
        .then(response => response.json())
        .then(data => {
           // console.log("allasdfghjklzxcvbnmUser: ", data);
           var htmlData = "";
            for(let i = 0 ; i < data.length ; i++) {
                htmlData = htmlData + `<a href="/message?id=${data[i].firstName}">${data[i].firstName}</a><br/>`;
             }
           //  console.log(htmlData);
           document.getElementById("user-list").innerHTML = htmlData;
        });
</script>

<body>
    {{>navbar}}

    <div id="user-list">

    </div>
    <script>


    </script>
</body>

</html>